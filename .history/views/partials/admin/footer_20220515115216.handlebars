    <script  type='text/javascript'>

        $("#approve").bootstrapSwitch();
        
        // them su kien cho ajax
        $(document).ready(()=>{

            $("#approve").on('switchChange.bootstrapSwitch', (e, data)=>{
            // lay ve id cua comment
            const id2 = $("#approve").attr('data-id');
            console.log(id2);
            
            $.ajax({
              type: "POST",
              url: '/admin/comments/approve-comment',
              data: {id: id2, approveComment: data},
              cache: false,
              success: function(data) {
                  toastr.success(`Comment ${data._id} đã được cập nhật`);
              }
            })


          })
        })

    </script>
