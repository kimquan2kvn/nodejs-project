<h1>ALL POSTS</h1>
<style>
table {
  background-color:#E7E9EB;
}
</style>
<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Status</th>
      <th>ALlow Comments</th>
    </tr>
  </thead>
  <tbody>
    {{#each posts}}
    <tr>
      <td>{{_id}}</td>
      <td>{{title}}</td>
      <td>{{status}}</td>
      <td>{{allowComments}}</td>
      <td> {{_id}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>